package chapter6;

import java.util.Scanner;

public class Lab6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char grade;
		int score = 91;
		
		Scanner input = new Scanner(System.in);
		
		if(score>= 90){
			grade = 'A';
		}else if(score >=80){
			grade = 'B';
		}else if(score >=70){
			grade = 'C';
		}else if(score >=60){
			grade = 'D';
		}else {
			grade = 'F';
		}
		
		System.out.println("The grade is " + grade);
	}

}
