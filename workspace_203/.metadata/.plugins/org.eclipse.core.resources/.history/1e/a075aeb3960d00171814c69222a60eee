package chapter6;

public class Lab6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char grade;
		int score = 91;
		
		if(score>= 90){
			grade = 'A';
		}else if(score >=80){
			grade = 'B';
		}else if(score >=70){
			grade = 'C';
		}else if(score >=60){
			grade = 'D';
		}else {
			grade = 'F';
		}
		

	}

}
