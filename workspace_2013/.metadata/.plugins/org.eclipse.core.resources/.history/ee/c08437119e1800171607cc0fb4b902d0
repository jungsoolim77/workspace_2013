package chapter6;

public class Lab6_2 {

	public static void main(String[] args) {
		int x = 5, y = 6;
		System.out.println(" x = "+ x + " y = " + y);
		swap(x,y);
		System.out.println(" x = "+ x + " y = " + y);

	}
	
	public static void swap(int x, int y){
		int temp = x;
		x = y;
		y = temp;
		System.out.println(" x = "+ x + " y = " + y);
}
	
