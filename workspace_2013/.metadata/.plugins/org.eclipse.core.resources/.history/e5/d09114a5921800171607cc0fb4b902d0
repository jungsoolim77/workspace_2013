package chapter6;

public class Lab6_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 5, y = 6;
		System.out.println(" x = "+ x + " y = " + y);

	}
	public static int max(int x, int y){
		return x>y? x:y;
	}
	public static void swap(int x, int y){
		int temp = x;
		x = y;
		y = temp;
		
	}

}
